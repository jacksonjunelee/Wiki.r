<%= @author.username %>

<ul>
<h2> List of documents made by <%= @author.username%> : </h2>
<% @documents.each do |document| %>
<%= document.title %><br>
<% end %>
</ul>


<ul>
<h2> List of changes made by <%= @author.username%> : </h2>
<% @documents.each do |document| %>
<%= document.title %> <span> | </span>
<%= document.updated_at %>
<% end %>
</ul>
